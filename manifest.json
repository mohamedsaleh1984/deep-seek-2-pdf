{
  "manifest_version": 3,
  "name": "DeepSeek chat to pdf",
  "version": "1.0",
  "description": "Free extension to help users to convert DeepSeek chat to pdf file.",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs"
  ],
  "action": {
    "default_popup": "DeepSeek2pdf.html",
    "default_icon": "pdf.png"
  },
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}